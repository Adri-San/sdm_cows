<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Toolbar -->
    <style name="Widget.Cows.Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="android:background">?attr/backgroundColor</item>
        <item name="android:theme">@style/Theme.COWS</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Dark</item>
    </style>
    <!-- FAB -->
    <style name="Widget.Cows.FloatingActionButton" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="backgroundTint">?attr/colorPrimary</item>
        <item name="tint">?attr/colorOnPrimary</item>
    </style>
</resources>